#import <Foundation/Foundation.h>

@interface HHSoftScrollImageInfo : NSObject
/**
 *  图片地址，网络图片是网络地址，本地图片是本地图片路径
 */
@property (nonatomic,copy) NSString *imageUrl;
/**
 *  是否是网络图片
 */
@property (nonatomic,assign) BOOL isWebImage;

@end
