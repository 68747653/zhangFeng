#import <Foundation/Foundation.h>
#import <UIKit/UIKit.h>

@interface HHSoftUploadImageInfo : NSObject
/**
 *  上传图片的ID（主要是用来修改的时候删除使用）
 */
@property (nonatomic,assign) NSInteger uploadImageID;
/**
 *  压缩图片
 */
@property (nonatomic,strong) UIImage *compressImage;
/**
 *  源图片
 */
@property (nonatomic,strong) UIImage *sourceImage;
/**
 *  压缩图片路径
 */
@property (nonatomic,strong) NSString *compressImageFilePath;
/**
 *  源图片路径
 */
@property (nonatomic,strong) NSString *sourceImageFilePath;

/**
 *  是否是网络图片
 */
@property (nonatomic,assign) BOOL isWebImage;

@end
